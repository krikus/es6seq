{
  "name": "seq6",
  "description": "Seq-like library for ES6",
  "version": "0.0.1b",
  "scripts": {
    "test": "node --harmony-classes `which gulp` test:coverage"
  },
  "devDependencies": {
    "chai": "^3.0.0",
    "git-guppy": "^1.0.0",
    "gulp": "^3.9.0",
    "gulp-codecov.io": "^1.0.1",
    "gulp-filter": "^2.0.2",
    "gulp-istanbul": "^0.10.0",
    "gulp-jshint": "^1.11.2",
    "gulp-mocha": "^2.1.3",
    "guppy-pre-commit": "^0.2.1",
    "istanbul": "^0.3.17",
    "jshint-stylish": "^2.0.1",
    "mocha": "^2.2.5"
  },
  "jshintConfig": {
    "esnext": true,
    "eqeqeq": true,
    "globalstrict": true,
    "strict": true,
    "unused": true,
